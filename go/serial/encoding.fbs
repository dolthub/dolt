namespace serial;

enum Encoding : uint8 {
  // fixed width
  Null     = 0,
  Int8     = 1,
  Uint8    = 2,
  Int16    = 3,
  Uint16   = 4,
  Int32    = 7,
  Uint32   = 8,
  Int64    = 9,
  Uint64   = 10,
  Float32  = 11,
  Float64  = 12,
  Bit64    = 13,
  Hash128  = 14,
  Year     = 15,
  Date     = 16,
  Time     = 17,
  Datetime = 18,
  Enum     = 19,
  Set      = 20,

  // variable width
  String   = 128,
  Bytes    = 129,
  Decimal  = 130,
  JSON     = 131,
  Geometry = 133,
}
